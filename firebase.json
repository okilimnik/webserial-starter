{
  "hosting": {
    "public": "resources/public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**",
      "resources/public/app-js/cljs-runtime",
      "resources/public/portfolio-js",
      "resources/public/portfolio.html",
      ".clj-kondo/.cache",
      ".cpcache",
      ".lsp",
      ".shadow-cljs",
      "resources/test",
      ".DS_Store"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
